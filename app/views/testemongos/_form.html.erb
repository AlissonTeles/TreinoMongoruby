<%= form_with do |form| %>
  

  <div class="form-floating">
    <%= form.label :username , class: "form-control"%>
    <%= form.text_field :username %>
  </div>
  <div class="form-floating">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>

  <div class="form-floating">
    <%= form.label :password %>
    <%= form.text_field :password %>
  </div>



  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>